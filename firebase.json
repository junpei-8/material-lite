{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/angular/**",
        "destination": "/angular/index.html",
        "type": 301
      },
      {
        "source": "/react/**",
        "destination": "/react/index.html",
        "type": 302
      }
    ]
  }
}
